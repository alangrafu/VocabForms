PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>

SELECT DISTINCT ?class (SAMPLE(?cl) AS ?classLabel) WHERE {
  GRAPH <{{lodspk.g}}>{
  	{
          ?class a owl:Class.
        }UNION{
          ?class a rdfs:Class.
        }
        OPTIONAL{ ?class rdfs:label ?cl}
  }
}GROUP BY ?class
